{
  "name": "awesome-laminar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate": "npx tsx scripts/generate.ts && npm run lib:fmt && npm run lib:fix",
    "generate:force": "npx tsx scripts/generate.ts --force && npm run lib:fmt && npm run lib:fix",
    "generate:verbose": "npx tsx scripts/generate.ts --verbose",
    "generate:validate": "npx tsx scripts/generate.ts --validate",
    "generate:ir": "tsx scripts/gen-ir.ts",
    "generate:types": "tsx scripts/extract-shared-types.ts",
    "generate:components": "tsx scripts/generate-components.ts",
    "fix": "./mill __.fix",
    "fmt": "./mill mill.scalalib.scalafmt/",
    "generate:facades": "rm -rf webawesome/src && ./mill webawesome.scalablyTypedImportTask",
    "compile:webawesome": "./mill webawesome.compile > compile-output.txt 2>&1 || true",
    "parse-override-errors": "npx tsx scripts/parse-override-errors.ts"
  },
  "workspaces": [
    "doc"
  ],
  "packageManager": "yarn@4.9.2",
  "devDependencies": {
    "@types/node": "^24.0.15",
    "code-block-writer": "^13.0.3",
    "jsdom": "^26.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@awesome.me/webawesome": "^3.0.0-beta.3"
  }
}
